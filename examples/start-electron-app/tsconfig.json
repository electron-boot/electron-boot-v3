{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "declaration": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "removeComments": true,
    "downlevelIteration": true,
    "inlineSources": true,
    "noLib": false,
    "useUnknownInCatchVariables": false,
    "target": "ES2021",
    "sourceMap": true,
    "allowJs": false,
    "resolveJsonModule": true,
    "baseUrl": "./"
  }
}
