{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "declaration": true,
    "module": "commonjs",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "removeComments": false,
    "downlevelIteration": true,
    "inlineSources": true,
    "noLib": false,
    "useUnknownInCatchVariables": false,
    "target": "ES2021",
    "sourceMap": true,
    "allowJs": false,
    "resolveJsonModule": true,
    "outDir": "dist",
    "baseUrl": "."
  }
}
