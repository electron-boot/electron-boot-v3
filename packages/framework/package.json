{
  "name": "@electron-boot/framework",
  "version": "2.0.3",
  "description": "fast development framework for electron",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
