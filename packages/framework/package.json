  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron-boot/electron-boot.git"
  },
  "keywords": [
    "electron",
    "bootstrap"
  ],
  "author": "electron-boot",
  "bugs": {
    "url": "https://github.com/electron-boot/electron-boot/issues"
  },
  "homepage": "https://github.com/electron-boot/electron-boot#readme",
  "license": "MIT",
  "dependencies": {
    "@electron-boot/logger": "workspace:*",
    "fs-extra": "^11.1.1",
    "picomatch": "^2.3.1"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@rollup-extras/plugin-clean": "^1.3.6",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash-es": "^4.17.9",
    "@types/picomatch": "^2.3.0",
    "electron": "^22.3.18",
    "lodash-es": "^4.17.21",
    "rollup": "^3.28.1",
    "rollup-plugin-dts": "^6.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": ">=5.2.2"
